{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["es2020"],
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": true,
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true
  }
}
